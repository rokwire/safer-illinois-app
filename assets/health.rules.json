	"statuses": {
		"PCR.xpositive": {
			"health_status": "red",
			"priority": 11,
			"next_step_html": "positive.step.html"
		},
		"PCR.positive": {
			"condition": "timeout",
			"params": {
				"interval": { "min": 0, "max": 10, "scope": "future" }
			},
			"success": {
				"condition": "require-test",
				"params": {
					"interval": { "min": 9, "max": null, "scope": "future" }
				},
				"success": null,
				"fail": {
					"health_status": "red",
					"priority": 11,
					"next_step": "test.now.step"
				}
			},
			"fail": {
				"health_status": "red",
				"priority": 11,
				"next_step_html": "positive.step.html"
			}
		},

		"PCR.positive-IP": {
			"condition": "timeout",
			"params": {
				"interval": { "min": 0, "max": 8, "scope": "future" }
			},
			"success": {
				"condition": "require-test",
				"params": {
					"interval": { "min": 9, "max": null, "scope": "future" }
				},
				"success": null,
				"fail": {
					"health_status": "red",
					"priority": 11,
					"next_step": "test.now.step",
